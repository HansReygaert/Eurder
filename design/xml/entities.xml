<?xml version="1.0" encoding="UTF-8" ?>
<xml>
<entities>
    <enums>
        <security-role>
            <unregistered></unregistered>
            <admin></admin>
        </security-role>
        <stock-level>
            <low></low>
            <medium></medium>
            <high></high>
        </stock-level>
    </enums>

    <customer>
        <firstName></firstName>
        <lastName></lastName>
        <email></email>
        <address></address>
        <phoneNumber></phoneNumber>
    </customer>

    <item>
        <id></id>
        <name></name>
        <description></description>
        <price></price>
        <amount></amount>
        <!-- Only admins can add items -->
    </item>

    <order>
        <order-id></order-id>
        <customer-id></customer-id>
        <item-group>
            <item-dto>
                <id></id>
                <amount></amount>
                <shipping-date>
                <!--
                    logic not in dto:
                    if in stock: +1 day
                    else: +7 days
                -->
                </shipping-date>
            </item-dto>
        </item-group>
        <total-price></total-price>
    </order>

    <report>
        <orders>
            <order-dto>
                <customer-id></customer-id>
                <order-id></order-id>
                <item-group></item-group>
                <total-price></total-price>
            </order-dto>
        </orders>
        <total-price></total-price>
    </report>


</entities>
</xml>

